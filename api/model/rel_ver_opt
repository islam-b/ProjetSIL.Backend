const Sequelize = require('sequelize');
const sequelize = require('../config/dbconnection');

const Rel_ver_opt = sequelize.define('rel_ver_opt', {
    idRelVerOpt: {type:Sequelize.INTEGER,primaryKey: true, autoIncrement: true},
    CodeVersion: {type:Sequelize.INTEGER},
    CodeOption: {type:Sequelize.STRING}
});
module.exports = Rel_ver_opt;
